{
  "name": "Export Quizlet to Vocabustudy",
  "description": "Converting Quizlet sets to Vocabustudy sets has never been easier",
  "version": "0.1",
  "permissions": ["declarativeNetRequestWithHostAccess"],
  "host_permissions": ["https://*.quizlet.com/*"],
  "declarative_net_request": {
    "rule_resources": [{
      "id": "ruleset_1",
      "enabled": true,
      "path": "dnr_rules.json"
    }]
  },
  "manifest_version": 3,
  "icons": {
    "128": "icons/icon-128.png",
    "48": "icons/icon-48.png",
    "32": "icons/icon-32.png",
    "16": "icons/icon-16.png"
  }
}