{
  "name": "__MSG_extName__",
  "description": "__MSG_extDesc__",
  "version": "0.5.0",
  "permissions": ["declarativeNetRequestWithHostAccess"],
  "host_permissions": ["https://quizlet.com/*"],
  "background": {
    "service_worker": "background.js"
  },
  "declarative_net_request": {
    "rule_resources": [{
      "id": "ruleset_1",
      "enabled": true,
      "path": "/dnr_rules.json"
    }]
  },
  "content_scripts": [
    {
      "matches": ["https://quizlet.com/*"],
      "js": ["/common/inject-convert-button.js"]
    }
  ],
  "manifest_version": 3,
  "icons": {
    "128": "/common/icons/icon-128.png",
    "48": "/common/icons/icon-48.png",
    "32": "/common/icons/icon-32.png",
    "16": "/common/icons/icon-16.png"
  },
  "externally_connectable": {
    "matches": [
      "https://vocabustudy.org/quizlet/*"
    ]
  },
  "action": {
    "default_icon": {
      "128": "/common/icons/icon-128.png",
      "48": "/common/icons/icon-48.png",
      "32": "/common/icons/icon-32.png",
      "16": "/common/icons/icon-16.png"
    },
    "default_title": "__MSG_actionTitle__"
  },
  "web_accessible_resources": [
    {
      "resources": ["/common/icons/icon-128.png"],
      "matches": ["https://quizlet.com/*"]
    }
  ],
  "default_locale": "en",
  "key": ""
}